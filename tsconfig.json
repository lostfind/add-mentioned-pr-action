{
  "compilerOptions": {
    "target": "ESNext",
    "module": "commonjs",
    "outDir": "./lib",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "strictPropertyInitialization": true,


  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
